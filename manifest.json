  "content_scripts": [
    {
      "matches": [
        "*://alunos.uminho.pt/pt/estudantes/paginas/infouteishorarios.aspx",
        "*://alunos.uminho.pt/EN/students/Pages/InfoUteisHorarios.aspx"
      ],
